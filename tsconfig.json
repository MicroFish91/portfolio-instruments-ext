{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"lib": [
			"ES2022"
		],
		"sourceMap": true,
        "rootDir": ".",
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strictNullChecks": true,
        "noUnusedParameters": true
	},
	"exclude": [
        "node_modules",
        ".vscode-test"
    ]
}
